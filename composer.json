{
    "name": "buepro/typo3-flux-elements",
    "type": "typo3-cms-extension",
    "description": "TYPO3 extension providing content elements powered by flux and bootstrap. Available elements: container, columns, tabs, accordion, tile unit and card.",
    "keywords": [
        "TYPO3 CMS",
        "flux",
        "grid",
        "structure",
        "element",
        "pizpalue"
    ],
    "homepage": "https://github.com/buepro/typo3-flux_elements",
    "authors": [
        {
            "name": "Roman BÃ¼chler",
            "email": "rb@buechler.pro",
            "homepage": "https://www.buechler.pro/",
            "role": "Developer"
        }
    ],
    "license": ["GPL-2.0-or-later"],
    "require": {
        "typo3/cms-core": "^9.5 || ^10",
        "fluidtypo3/flux": "^9"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Buepro\\FluxElements\\": "Classes"
        }
    },
    "replace": {
        "flux_elements": "self.version",
        "typo3-ter/flux-elements": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "flux_elements"
        }
    }
}
